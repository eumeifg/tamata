                <argument name="zone" xsi:type="string">sales</argument>
            </arguments>
        </block>
        <block class="Magedelight\Tax\Block\Sellerhtml\Item\Price\Renderer" name="item_row_total_after_discount" template="Magedelight_Sales::vendor/order/items/price/total_after_discount.phtml"/>
    </body>
