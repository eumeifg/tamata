<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <commission>
            <payout>
                <shipping_liability>2</shipping_liability>
                <tax_liability>2</tax_liability>
                <duration>15</duration>
            </payout>
            <payout_commission>
                <precedence_1>1</precedence_1>
                <precedence_2>2</precedence_2>
                <precedence_3>3</precedence_3>
            </payout_commission>
        </commission>
        <emailconfiguration>
            <vendor_comm>
                <enabled>1</enabled>
                <template>emailconfiguration_vendor_comm_template</template>           
            </vendor_comm>
            <vendor_commission_new>
                <enabled>1</enabled>
                <template>emailconfiguration_vendor_commission_new_template</template>           
            </vendor_commission_new>
            <vendor_catcommission_new>
                <enabled>1</enabled>
                <template>emailconfiguration_vendor_catcommission_new_template</template>           
            </vendor_catcommission_new>
            <vendor_commission_change>
                <enabled>1</enabled>
                <template>emailconfiguration_vendor_commission_change_template</template>           
            </vendor_commission_change>
            <global_commission_change>
                <enabled>1</enabled>
                <template>emailconfiguration_global_commission_change_template</template>           
            </global_commission_change>
            <vendor_catcommission_change>
                <enabled>1</enabled>
                <template>emailconfiguration_vendor_catcommission_change_template</template>           
            </vendor_catcommission_change>
            <vendor_product_commission_change>
                <enabled>1</enabled>
                <template>emailconfiguration_vendor_product_commission_change_template</template>           
            </vendor_product_commission_change>
        </emailconfiguration>
    </default>
</config>
