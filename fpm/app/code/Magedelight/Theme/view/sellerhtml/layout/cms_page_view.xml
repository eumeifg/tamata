<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <move element="page.main.title" destination="vendor-cms-wrapper" before="cms_page"/>
        <referenceContainer name="content">
            <container name="vendor-cms-wrapper" label="Vendor CMS Wrapper" as="vendor-cms-wrapper" htmlTag="div" htmlClass="vendor-cms-wrapper">
                <block class="Magento\Cms\Block\Page" name="cms_page"/>
            </container>
        </referenceContainer>
    </body>
</page>
