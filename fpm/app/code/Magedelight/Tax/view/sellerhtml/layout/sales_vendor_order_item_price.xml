<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <block class="Magedelight\Tax\Block\Sellerhtml\Item\Price\Renderer" name="item_unit_price" template="Magento_Tax::item/price/unit.phtml">
            <arguments>
                <argument name="zone" xsi:type="string">sales</argument>
            </arguments>
        </block>
        <block class="Magedelight\Tax\Block\Sellerhtml\Item\Price\Renderer" name="item_row_total" template="Magento_Tax::item/price/row.phtml">
