<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="footer">
             <container name="footer.panel.wrapper" htmlClass="panel wrapper container footer-links-container" htmlTag="div">
               <container name="footer.panel.wrapper.content" htmlClass="panel row" htmlTag="div">
                    <block class="Magento\Cms\Block\Block" name="vendor-footer-links">
                        <arguments>
                            <argument name="block_id" xsi:type="string">vendor-footer-links</argument>
                        </arguments>
                    </block>
                </container>
            </container>
        </referenceContainer>
    </body>
</page>
